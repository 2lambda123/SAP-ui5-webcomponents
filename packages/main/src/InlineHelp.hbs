<div style="display: inline-block" class="ui5-popover-trigger" tabindex="0">
    {{#if _trigger}}
        <slot></slot>
    {{else}}
        <ui5-icon name="{{icon}}" tabindex="0"></ui5-icon>
    {{/if}}
</div>

<ui5-popover placement-type="{{placementType}}">
    {{#if text}}
        {{ text }}
    {{else}}
        <slot name="content"></slot>
    {{/if}}
</ui5-popover>
